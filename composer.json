{
    "name": "browncat-nl/healthcheck-bundle",
    "description": "Tool for health checks",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Lennart Voogd",
            "email": "lennart@browncat.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "psr/log": "^1.1",
        "symfony/config": "^4.4.3|^5.0|^6.0",
        "symfony/dependency-injection": "^4.4.3|^5.0|^6.0",
        "symfony/framework-bundle": "^4.4.3|^5.0|^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Browncat\\HealthCheckBundle\\": "src/"
        }
    },
    "require-dev": {
        "doctrine/coding-standard": "^9.0",
        "doctrine/doctrine-bundle": "^2.3",
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.5",
        "psalm/plugin-phpunit": "^0.16.0",
        "psalm/plugin-symfony": "^2.3",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/dotenv": "^4.4.3|^5.0|^6.0",
        "vimeo/psalm": "^4.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Browncat\\HealthCheckBundle\\Tests\\": "tests/"
        }
    }
}
